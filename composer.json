{
    "name": "nanasess/eccube2-fixture-generator",
    "type": "library",
    "require-dev": {
        "symfony/console": "^2.8 || ^3.4 || ^4.4 || ^5.4 || ^6.4 || ^7.0",
        "friendsofphp/php-cs-fixer": "^3.65"
    },
    "license": "LGPL-3.0",
    "autoload": {
        "files": ["bootstrap.php"],
        "psr-4": {
            "Eccube2\\Tests\\Fixture\\": "tests/Fixture"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Eccube2\\Command\\": "src/Command"
        }
    },
    "suggest": {
        "ec-cube2/cli": "Required if using the cli interface."
    },
    "authors": [
        {
            "name": "Kentaro Ohkouchi",
            "email": "nanasess@fsm.ne.jp"
        }
    ],
    "require": {
        "fakerphp/faker": "^1.23"
    }
}
